		else
			if limite siis 2 then</pc>
				<li><pc>write limite</pc></li>
			<pc>end
		else
			whila(numerito <= limite) then
				for i in summon range(2,numerito)
					if (numerito % i siis 0) then
						flag is 1
						break
					end
				end
				if flag siis 0 then
					if numerito siis 3 then
						count is count + 1</pc>
						<li><pc>write 2</pc></li>
					<pc>end
					primo_actual is numerito
					count is count + 1</pc>
					<li><pc>write primo_actual</pc></li>
					<pc>end
				numerito is numerito + 1
				flag is 0
			end
		end
	end
end</pc>


